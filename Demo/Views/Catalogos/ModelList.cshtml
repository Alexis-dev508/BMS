@model Demo.Models.EquiposModelView
<option value="">--Seleccionar Modelo--</option>
@if (ViewBag.ModelList != null && ViewBag.Count != 0)
{
	foreach(var item in ViewBag.ModelList)
	{
		<option value="@item.Value">@item.Text</option>
}
	if (ViewBag.modelo != null && ViewBag.nombremodelo != null)
	{
		<option value="@ViewBag.modelo" selected="selected">@ViewBag.nombremodelo</option>
	}
}
else
{
	<option value="" selected="selected">No aplica</option>
}
