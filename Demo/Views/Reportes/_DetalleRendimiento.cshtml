@model IEnumerable<Demo.Models.DetalleRendimiento>
<div class="container-fluid">
	<div class="row">
		<div class="col-sm-12">
			<div class="card-primary">
				<div class="card-header">
				   <h3>Detalles</h3>
				</div>
				<div class="table-responsive table-bordered">
					<table class="table table-hover" id="tabla">
					<thead>
					 <tr>
					  <th>Folio</th>
					  <th>Fecha</th>
					  <th>Operador</th>
					  <th>Nombre</th>
					  <th>Lectura</th>
					  <th>Cantidad</th>
					  <th>Importe</th>
					  <th>Rend/Can</th>
					  <th>Ren Acum</th>
					  <th>Notas servicio</th>
					  <th>Notas</th>
				  </tr>
				 </thead>
				 <tbody>
					 @foreach(var dat in Model)
						{
							<tr>
					   <td>@dat.folio</td>
					   <td>@dat.fecha</td>
					   <td>@dat.operador</td>
					   <td>@dat.nombre</td>
					   <td>@dat.lectura</td>
						<td>@dat.cantidad</td>
						<td>@dat.importe</td>
						<td>@dat.rend_can</td>
						<td>@dat.rend_acum</td>
						<td>@dat.notasservicio</td>
						<td>dat.notas</td>
				   </tr>
						}
				 </tbody>
				</table>
				</div>
				
			</div>
		</div>
	</div>
</div>
<script>
$(document).ready( function () {
    $('#tabla').DataTable();
} );
</script>
